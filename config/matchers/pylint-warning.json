{
    "problemMatcher": [
        {
            "owner": "pylint-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(\\d+): ([^EF][0-9]+): (.*?) \\((\\S+)\\)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 5,
                    "code": 4
                }
            ]
        }
    ]
}
