{
    "problemMatcher": [
        {
            "owner": "pylint-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(\\d+): ([E|F][0-9]+): (.*?) \\((\\S+)\\)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 5,
                    "code": 4
                }
            ]
        }
    ]
}
