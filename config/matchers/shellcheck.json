{
    "problemMatcher": [
        {
            "owner": "shellcheck",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):(\\d+): (\\w+): (.*?) \\[(\\w+)\\]$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}
